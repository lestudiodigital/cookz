{
  "name": "cookz",
  "version": "0.0.4",
  "author": "antoninlanglade",
  "description": "Cookies Manager",
  "homepage": "http://lestudiodigital.github.io/cookz",
  "scripts": {
    "build": "parcel build index.js",
    "start": "parcel serve --port 3000 index.html",
    "predeploy": "parcel build index.html --out-dir ./cookz --public-url /cookz/",
    "deploy": "gh-pages -d cookz"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "source": [
    "index.js",
    "index.html"
  ],
  "license": "MIT",
  "targets": {
    "main": {
      "node": ">=4.x",
      "browsers": [
        "> 1%",
        "not dead"
      ]
    },
    "browser": {
      "browsers": [
        "> 1%",
        "not dead"
      ]
    }
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "parcel": "latest",
    "sass": "^1.23.7"
  },
  "dependencies": {
    "browser-cookies": "^1.2.0",
    "classnames": "^2.2.6",
    "crel": "^4.2.0",
    "src": "^1.1.2"
  }
}
